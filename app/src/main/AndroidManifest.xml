    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SampleDatabinding">
        <activity android:name="com.vikskod.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.vikskod.databinding.MainActivity" />
        <activity android:name="com.vikskod.viewmodel.MainActivity" />
        <activity android:name="com.vikskod.viewmodellivedata.MainActivity" />
        <activity android:name="com.vikskod.twowaydatabinding.MainActivity" />
        <activity android:name="com.vikskod.navigationcomponent.MainActivity" />
        <activity android:name="com.vikskod.coroutinesviewmodelscope.MainActivity" />
        <activity android:name="com.vikskod.roomandall.MainActivity" />

    </application>

</manifest>